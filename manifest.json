{
  "manifest_version": 2,

  "name": "EksiSayac",
  "description": "Sözlükteki girdi sıra sayısını gösteren eklentidir",
  "version": "1.0.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ],

  "content_scripts": [ {
    "js": [ "jquery.js", "eksisayac.js" ],
    "matches": [ "http://eksisozluk.com/*", "https://eksisozluk.com/*" ]
  }]
}
